            "lastdata": {
                "class": "FLOW_LASTDATA",
                "endpoint": "LASTDATA",
                "condition": {
                    "attrs": [
                        "TimeInstant",
                        "sceneRef",
                        "sourceRef",
                        "trend",
                        "dayType",
                        "hour",
                        "minute"
                    ],
                    "alterationTypes": [
