{
    "RouteIntensity": {
        "description": "Intensidad de uso ruta. Describe el número de viajeros \nde una ruta dada una estacionalidad, tipo de día y hora.",
        "namespace": "dtwin",
        "exampleId": "C1",
        "model": {
            "TimeInstant": {
                "description": "Fecha / Hora del cálculo de vista identidad o simulación",
                "ngsiType": "DateTime",
                "dbType": "timestamp with time zone NOT NULL",
                "extra": "-",
                "unit": "-",
                "flows": [
                    "historic",
                    "lastdata"
                ],
                "example": "2018-12-10T20:40:23",
                "range": "-"
            },
            "sourceRef": {
                "description": "ID de entidad original.\nReemplaza al entityId en a base de datos, ya que esta entidad\nes *singleton* y su ID en base de datos se ve sobrescrito\npor una composición de los campos únicos.",
                "ngsiType": "TextUnrestricted",
                "dbType": "text",
                "extra": "-",
                "unit": "-",
                "flows": [
                    "historic",
                    "lastdata"
                ],
                "example": "Parking-01",
                "range": "-"
            },
            "sceneRef": {
                "description": "ID del escenario de simulación.\nIdentifica la simulación realizada. El valor \"NA\"\nindica que se trata de una vista identidad.",
                "ngsiType": "TextUnrestricted",
                "dbType": "text",
                "extra": "-",
                "unit": "-",
                "flows": [
                    "historic"
                ],
                "example": "example text",
                "range": "-"
            },
            "trend": {
                "description": "Estacionalidad o tendencia para la que se ha calculado el escenario",
                "ngsiType": "TextUnrestricted",
                "dbType": "text",
                "extra": "-",
                "unit": "-",
                "flows": [
                    "historic"
                ],
                "example": "Verano",
                "range": "Verano, Fallas, Otros"
            },
            "dayType": {
                "description": "Tipo de día al que corresponde la medida",
                "ngsiType": "TextUnrestricted",
                "dbType": "text",
                "extra": "-",
                "unit": "-",
                "flows": [
                    "historic"
                ],
                "example": "L-J",
                "range": "L-J, Viernes, Sábado, Domingo"
            },
            "name": {
                "description": "Nombre descriptivo de la entidad",
                "ngsiType": "TextUnrestricted",
                "dbType": "text",
                "extra": "-",
                "unit": "-",
                "flows": [
                    "lastdata"
                ],
                "example": "example text",
                "range": "-"
            },
            "zoneList": {
                "description": "Lista de identificadores de la zona o distrito a la que pertenece la entidad",
                "ngsiType": "Json",
                "dbType": "json",
                "extra": "-",
                "unit": "-",
                "example": [
                    "Distrito 1",
                    "Distrito 4"
                ],
                "flows": [
                    "lastdata"
                ],
                "range": "-"
            },
            "forwardTrips": {
                "description": "Número de trayectos de ida",
                "ngsiType": "Number",
                "dbType": "double precision",
                "extra": "-",
                "unit": "-",
                "flows": [
                    "historic"
                ],
                "example": 5.0,
                "range": "-"
            },
            "returnTrips": {
                "description": "Número de trayectos de vuelta",
                "ngsiType": "Number",
                "dbType": "double precision",
                "extra": "-",
                "unit": "-",
                "flows": [
                    "historic"
                ],
                "example": 5.0,
                "range": "-"
            },
            "forwardStops": {
                "description": "Número de paradas en el trayecto de ida",
                "ngsiType": "Number",
                "dbType": "int",
                "extra": "-",
                "unit": "-",
                "flows": [
                    "historic"
                ],
                "example": 5,
                "range": "-"
            },
            "returnStops": {
                "description": "Número de paradas en el trayecto de vuelta",
                "ngsiType": "Number",
                "dbType": "int",
                "extra": "-",
                "unit": "-",
                "flows": [
                    "historic"
                ],
                "example": 5,
                "range": "-"
            },
            "location": {
                "description": "Ubicación de la entidad",
                "ngsiType": "geo:json",
                "dbType": "geometry(MultiLineString)",
                "extra": "-",
                "unit": "-",
                "flows": [
                    "lastdata"
                ],
                "example": {
                    "type": "geo:json",
                    "value": {
                        "type": "Line",
                        "coordinates": [
                            [
                                [
                                    3.5,
                                    24.6
                                ],
                                [
                                    33,
                                    44
                                ]
                            ]
                        ]
                    }
                },
                "range": "-"
            },
            "intensity": {
                "description": "Número de viajeros",
                "ngsiType": "Number",
                "dbType": "double precision",
                "extra": "-",
                "unit": "-",
                "flows": [
                    "historic"
                ],
                "example": 5.0,
                "range": "-"
            }
        },
        "flows": {
            "historic": {
                "class": "FLOW_HISTORIC",
                "endpoint": "HISTORIC",
                "pk": [
                    "entityid",
                    "timeinstant",
                    "sceneRef",
                    "trend",
                    "dayType"
                ],
                "dbIndexes": {
                    "scene": "(timeinstant, sceneRef)"
                },
                "condition": {
                    "attrs": [
                        "sourceRef",
                        "TimeInstant",
                        "sceneRef",
                        "trend",
                        "dayType"
                    ],
                    "expression": {
                        "q": "sceneRef"
                    }
                },
                "replaceId": [
                    "sourceRef"
                ]
            },
            "lastdata": {
                "class": "FLOW_LASTDATA",
                "endpoint": "LASTDATA",
                "condition": {
                    "attrs": [
                        "TimeInstant",
                        "sourceRef"
                    ],
                    "alterationTypes": [
                        "entityUpdate",
                        "entityCreate"
                    ]
                },
                "replaceId": [
                    "sourceRef"
                ]
            },
            "join": {
                "class": "FLOW_JOIN_VIEW",
                "leftModel": {
                    "name": "routeintensity",
                    "entityNamespace": "dtwin",
                    "attrs": [
                        "entityid",
                        "entityType",
                        "fiwareservicepath",
                        "recvtime",
                        "timeinstant",
                        "sourceref",
                        "sceneref",
                        "trend",
                        "daytype",
                        "forwardtrips",
                        "returntrips",
                        "forwardstops",
                        "returnstops",
                        "intensity"
                    ],
                    "attrJoinOn": [
                        "entityid"
                    ]
                },
                "rightModel": [
                    {
                        "name": "routeintensity_lastdata",
                        "entityNamespace": "dtwin",
                        "attrs": [
                            "name",
                            "location",
                            "zonelist"
                        ],
                        "attrJoinOn": [
                            "entityid"
                        ]
                    }
                ]
            },
            "custom_sql": {
                "class": "FLOW_RAW",
                "sql": {
                    "sources": {
                        "custom_sql": {
                            "documentation": "Conjunto de vistas utilitarias para la presentación de\ndatos de escenarios identidad y simulaciones.",
                            "path": "./sql",
                            "files": [
                                "custom_RouteIntensity.sql"
                            ],
                            "weight": 10,
                            "relations": []
                        }
                    }
                }
            },
            "sim": {
                "class": "FLOW_JOIN_VIEW",
                "leftModel": {
                    "name": "simulation_lastdata",
                    "entityNamespace": "dtwin",
                    "attrs": [
                        "timeinstant"
                    ],
                    "attrJoinOn": [
                        "entityId",
                        "timeinstant"
                    ]
                },
                "rightModel": [
                    {
                        "name": "routeintensity_join",
                        "entityNamespace": "dtwin",
                        "attrs": [
                            "entityid",
                            "entitytype",
                            "recvtime",
                            "fiwareservicepath",
                            "sourceref",
                            "sceneref",
                            "trend",
                            "daytype",
                            "name",
                            "location",
                            "zonelist",
                            "forwardtrips",
                            "returntrips",
                            "forwardstops",
                            "returnstops",
                            "intensity"
                        ],
                        "attrJoinOn": [
                            "sceneRef",
                            "timeinstant"
                        ]
                    }
                ]
            }
        }
    }
}
