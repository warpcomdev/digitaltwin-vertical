{
  "AirQualityObserved": {
    "namespace": "dtwin",
    "exampleId": "C1",
    "description": "Calidad del aire observada.",
    "model": {
      "TimeInstant": {
        "description": "Fecha / Hora del cálculo de vista identidad o simulación",
        "ngsiType": "DateTime",
        "dbType": "timestamp with time zone NOT NULL",
        "extra": "-",
        "unit": "-",
        "flows": [
          "historic",
          "lastdata"
        ],
        "example": "2018-12-10T20:40:23",
        "range": "-"
      },
      "sourceRef": {
        "description": "ID de entidad original.\nReemplaza al entityId en a base de datos, ya que esta entidad\nes *singleton* y su ID en base de datos se ve sobrescrito\npor una composición de los campos únicos.",
        "ngsiType": "TextUnrestricted",
        "dbType": "text",
        "extra": "-",
        "unit": "-",
        "flows": [
          "historic",
          "lastdata"
        ],
        "example": "Parking-01",
        "range": "-"
      },
      "sceneRef": {
        "description": "ID del escenario de simulación.\nIdentifica la simulación realizada. El valor \"N/A\"\nindica que se trata de una vista identidad.",
        "ngsiType": "TextUnrestricted",
        "dbType": "text",
        "extra": "-",
        "unit": "-",
        "flows": [
          "historic",
          "lastdata"
        ],
        "example": "example text",
        "range": "-"
      },
      "trend": {
        "description": "Estacionalidad o tendencia para la que se ha calculado el escenario",
        "ngsiType": "TextUnrestricted",
        "dbType": "text",
        "extra": "-",
        "unit": "-",
        "flows": [
          "historic"
        ],
        "example": "Verano",
        "range": "Verano, Fallas, Otros"
      },
      "dayType": {
        "description": "Tipo de día al que corresponde la medida",
        "ngsiType": "TextUnrestricted",
        "dbType": "text",
        "extra": "-",
        "unit": "-",
        "flows": [
          "historic"
        ],
        "example": "L-J",
        "range": "L-J, Viernes, Sábado, Domingo"
      },
      "name": {
        "description": "Nombre descriptivo de la entidad",
        "ngsiType": "TextUnrestricted",
        "dbType": "text",
        "extra": "-",
        "unit": "-",
        "flows": [
          "lastdata"
        ],
        "example": "example text",
        "range": "-"
      },
      "zone": {
        "description": "Identificador de la zona o distrito a la que pertenece la entidad",
        "ngsiType": "TextUnrestricted",
        "dbType": "text",
        "extra": "-",
        "unit": "-",
        "example": "Distrito 1",
        "flows": [
          "lastdata"
        ],
        "range": "-"
      },
      "NO2": {
        "description": "Dióxido de Nitrógeno",
        "ngsiType": "Number",
        "dbType": "double precision",
        "extra": "-",
        "unit": "µg/m3",
        "flows": [
          "historic"
        ],
        "example": 5.0,
        "range": "-"
      },
      "PM25": {
        "description": "Partículas en suspensión inferiores a 2,5 micras",
        "ngsiType": "Number",
        "dbType": "double precision",
        "extra": "-",
        "unit": "µg/m3",
        "flows": [
          "historic"
        ],
        "example": 5.0,
        "range": "-"
      },
      "PM10": {
        "description": "Dióxido de Nitrógeno",
        "ngsiType": "Number",
        "dbType": "double precision",
        "extra": "-",
        "unit": "µg/m3",
        "flows": [
          "historic"
        ],
        "example": 5.0,
        "range": "-"
      },
      "location": {
        "description": "Ubicación de la entidad",
        "ngsiType": "geo:json",
        "dbType": "geometry(Point)",
        "extra": "-",
        "unit": "-",
        "flows": [
          "lastdata"
        ],
        "example": {
          "type": "geo:json",
          "value": {
            "type": "Point",
            "coordinates": [
              3.5,
              24.6
            ]
          }
        },
        "range": "-"
      },
      "O3": {
        "description": "Ozono",
        "ngsiType": "Number",
        "dbType": "double precision",
        "extra": "-",
        "unit": "µg/m3",
        "flows": [
          "historic"
        ],
        "example": 5.0,
        "range": "-"
      }
    },
    "flows": {
      "historic": {
        "class": "FLOW_HISTORIC",
        "endpoint": "HISTORIC",
        "pk": [
          "entityid",
          "timeinstant",
          "sceneRef",
          "trend",
          "dayType"
        ],
        "dbIndexes": {
          "scene": "(timeinstant, sceneRef)"
        },
        "condition": {
          "attrs": [
            "sourceRef",
            "TimeInstant",
            "sceneRef",
            "trend",
            "dayType"
          ],
          "expression": {
            "q": "sceneRef"
          }
        },
        "replaceId": [
          "sourceRef"
        ]
      },
      "lastdata": {
        "class": "FLOW_LASTDATA",
        "endpoint": "LASTDATA",
        "condition": {
          "attrs": [
            "TimeInstant",
            "sourceRef"
          ],
          "alterationTypes": [
            "entityUpdate",
            "entityCreate"
          ],
          "expression": {
            "q": "sceneRef==N/A"
          }
        },
        "replaceId": [
          "sourceRef"
        ]
      },
      "join": {
        "class": "FLOW_JOIN_VIEW",
        "leftModel": {
          "name": "airqualityobserved",
          "entityNamespace": "dtwin",
          "attrs": [
            "entityid",
            "entityType",
            "fiwareservicepath",
            "recvtime",
            "timeinstant",
            "sourceref",
            "sceneref",
            "trend",
            "daytype",
            "no2",
            "pm25",
            "pm10",
            "o3"
          ],
          "attrJoinOn": [
            "entityid"
          ]
        },
        "rightModel": [
          {
            "name": "airqualityobserved_lastdata",
            "entityNamespace": "dtwin",
            "attrs": [
              "name",
              "location",
              "zone"
            ],
            "attrJoinOn": [
              "entityid"
            ]
          }
        ]
      },
      "custom_sql": {
        "class": "FLOW_RAW",
        "sql": {
          "sources": {
            "custom_sql": {
              "documentation": "Conjunto de vistas utilitarias para la presentación de\ndatos de escenarios identidad y simulaciones.",
              "path": "./sql",
              "files": [
                "custom_AirQualityObserved.sql"
              ],
              "weight": 10,
              "relations": []
            }
          }
        }
      },
      "sim": {
        "class": "FLOW_JOIN_VIEW",
        "leftModel": {
          "name": "simulation_lastdata",
          "entityNamespace": "dtwin",
          "attrs": [
            "timeinstant"
          ],
          "attrJoinOn": [
            "entityId",
            "timeinstant"
          ]
        },
        "rightModel": [
          {
            "name": "airqualityobserved_join",
            "entityNamespace": "dtwin",
            "attrs": [
              "entityid",
              "entitytype",
              "recvtime",
              "fiwareservicepath",
              "sourceref",
              "sceneref",
              "trend",
              "daytype",
              "name",
              "location",
              "zone",
              "no2",
              "pm25",
              "pm10",
              "o3"
            ],
            "attrJoinOn": [
              "sceneRef",
              "timeinstant"
            ]
          }
        ]
      }
    }
  }
}