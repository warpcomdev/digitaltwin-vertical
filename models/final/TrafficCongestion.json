      "minute": {
        "description": "Intervalo de 10 minutos al que corresponde la medida",
        "ngsiType": "Number",
        "dbType": "int",
        "extra": "-",
        "unit": "-",
        "flows": [
          "historic"
        ],
        "range": "0-50",
        "example": 20
      },
      "zone": {
        "description": "Identificador de la zona o distrito a la que pertenece la entidad",
