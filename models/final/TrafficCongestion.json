            "entityType",
            "fiwareservicepath",
            "recvtime",
            "timeinstant",
            "sourceref",
            "sceneref",
            "trend",
            "daytype",
            "hour",
            "minute",
            "congestion"
          ],
          "attrJoinOn": [
            "entityid"
          ]
        },
        "rightModel": [
          {
            "name": "trafficcongestion_lastdata",
            "entityNamespace": "dtwin",
            "attrs": [
              "name",
              "location",
              "zone"
            ],
            "attrJoinOn": [
              "entityid"
            ]
          }
        ]
      },
      "custom_sql": {
