          "documentation": "Subscripción del flujo historic (tipo FLOW_HISTORIC) en modelo OffStreetParking",
          "description": "OffStreetParking:HISTORIC:dtwin:historic",
          "status": "active",
          "subject": {
            "entities": [
              {
                "idPattern": ".*",
                "type": "OffStreetParking"
              }
            ],
            "condition": {
              "attrs": [
                "sourceRef",
                "TimeInstant",
                "sceneRef",
                "trend",
                "dayType",
                "hour"
              ],
              "expression": {
                "q": "sceneRef"
              }
            }
          },
          "notification": {
            "attrs": [
              "TimeInstant",
              "sourceRef",
              "sceneRef",
              "trend",
              "dayType",
              "hour",
              "capacity",
              "occupationPercent",
              "occupation"
            ],
            "httpCustom": {
              "url": "HISTORIC",
              "headers": {
                "fiware-servicepath": "/dtwin"
              },
              "ngsi": {
                "id": "${sourceRef}"
              }
            }
          }
        }
      }
    },
