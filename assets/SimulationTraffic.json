{
  "SimulationTraffic": {
    "etl_vectorize": {
      "subscriptions": {
        "trigger_etl": {
          "documentation": "Subscripci√≥n del flujo etl_vectorize (tipo FLOW_RAW) en modelo SimulationTraffic",
          "description": "SimulationTraffic:JENKINS::etl_vectorize_run",
          "status": "active",
          "subject": {
            "entities": [
              {
                "idPattern": ".*",
                "type": "SimulationTraffic"
              }
            ],
            "condition": {
              "attrs": [
                "TimeInstant"
              ],
              "alterationTypes": [
                "entityCreate",
                "entityUpdate",
                "entityChange",
                "entityDelete"
              ]
            }
          },
          "notification": {
            "attrs": [
              "TimeInstant"
            ],
            "httpCustom": {
              "url": "JENKINS/etl_digitaltwin_vectorize/buildWithParameters",
              "headers": {
                "Authorization": "Basic !!{JENKINS_BASIC_AUTH}"
              },
              "qs": {
                "ETL_VECTORIZE_SIMULATION_TYPE": "${type}",
                "ETL_VECTORIZE_SIMULATION_ID": "${id}",
                "ETL_VECTORIZE_CHANGETYPE": "${alterationType}"
              },
              "payload": null
            }
          }
        }
      }
    }
  }
}